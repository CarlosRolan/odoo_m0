    </record>

    <record id="menu_ent0_create_website" model="website.menu">
        <field name="name">Create new entry of Ent0</field>
        <field name="url">/ent0_create</field>
        <field name="parent_id" ref="menu_ent0_website" />
        <field name="sequence" type="int">50</field>
    </record>

</odoo>
